<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js " > <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js " > <![endif]-->
<!--[if !IE]><!-->
<html lang="en" class="no-js" ng-app="myApp">
<!--<![endif]-->
<!-- BEGIN HEAD -->

<head>
    <meta charset="utf-8" />
    <title>Online PDF tools | PDF To Jpg</title>
    <meta http-equiv="content-language" content="en_EN" />
    <link href="/css/love13.css" rel="stylesheet" type="text/css" />
    <script src="/js/jquery.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/js/angular.min.js"></script>
    <script src="/js/ng-file-upload-shim.min.js"></script>
    <script src="/js/ng-file-upload.min.js"></script>
    <script src="/js/FileSaver.min.js"></script>
    <script src="/js/app.js" />
    </script>
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->

<body
    class="page-header-fixed page-full-width page-footer-fixed LGen index cactionindex brwFirefox ver54 mob systemWindows app"
    data-ng-controller="AppCtrl" id="body">

    <!-- BEGIN HEADER -->
    <div class="header navbar navbar-fixed-top" id="header">
        <!-- BEGIN TOP NAVIGATION BAR -->
        <div class="header-inner">

            <a class="navbar-brand" href="/" title="Back to homepage">
                <img src="/images/logo.png" width="147" class="img-responsive">
            </a>


        </div>
        <div class="navbar navbar-default" id="navigation">

            <div class="navbar-header">
                <button type="button" data-target="#navigation-inner" data-toggle="collapse" class="navbar-toggle">
                    <span class="sr-only">Menu</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div id="navigation-inner" class="collapse navbar-collapse">
                <ul class="nav nav-justified">
                    <li><a href="/image_to_pdf">IMAGE to PDF</a></li>
                    <li><a href="/word_to_pdf">WORD to PDF</a></li>
                    <li><a href="/powerpoint_to_pdf">POWERPOINT to PDF</a></li>
                    <li> <a href="/excel_to_pdf">EXCEL to PDF</a></li>
                    <li class="active"><a href="/pdf_to_jpg">PDF to JPG</a></li>
                    <li><a href="/pdf_to_word">PDF to Word</a></li>
                </ul>
            </div>
        </div>
    </div>
    <!-- END HEADER -->
    <!-- END HEADER -->
    <div class="clearfix"></div>
    <!-- BEGIN CONTAINER -->

    <div class="page-content-wrapper">

        <!-- BEGIN PAGE -->
        <div class="page-content">
            <span style="display: none" data-ng-init="url = '/pdf_to_jpg'; convertUrl = '/convert_pdf_to_jpg'"></span>
            <div class="col-md-12">

                <!-- BEGIN PAGE TITLE & BREADCRUMB-->
                <h1 class="page-title tool">
                    Convert PDF to JPG </h1>

                <h2 class="page-subtitle">Make PDF Files easy to read by converting them to JPEG.</h2>
                <hr class="h1">
                <div id="upload-container">
                    <a type="button" class="btn redlove btn-xl" id="pickfiles" data-ng-if="!state.actions"
                        data-ng-click="state.select();" href="javascript:;">Select PDF file</a>

                    <br />
                </div>
            </div>

            <div id="fileGroups" class="margin-top-20" data-ng-show="state.preview">
                <div class="file tooltips pdfjpg" data-ng-repeat="file in state.files" rel="user" data-placement="top"
                    data-original-title="17 KB">
                    <a class="btn btn-icon-only grey remove tooltips "
                        data-ng-click="state.close($event.currentTarget,file)" style="display: initial"
                        data-placement="top" data-original-title="Remove this file">
                        <i class="fa fa-times"></i>
                    </a>
                    <canvas class="covercanvas" id="cover-o_1bp1f5n7s1sk32t31gqj1u341b6ha" width="150"
                        height="212"></canvas>
                    <div class="fileInfo">
                        <span class="filename" style="margin-left: auto; word-wrap: break-word">{{file.name}}</span>
                    </div>
                </div>
            </div>
            <form class="hide">
                <input id="file" type="file" ngf-select="state.uploadFiles($files, $invalidFiles)"
                    accept="application/pdf" />
            </form>
            <!-- END PAGE CONTENT-->
        </div>
        <!-- END PAGE -->

    </div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    <div class="footer" style="background-color: rgb(255, 255, 255);">
        <div class="footer-button redlove" data-ng-show="state.actions" id="footer-button" style="display: block;">
            <a type="button" id="uploadfiles" href="javascript:;" data-ng-click="state.convert(state.files)"
                class="btn black btn-xl"> Convert to JPG </a>
        </div>
        <div class="footer-button redlove" data-ng-show="((state.download === true) && (state.actions == false))"
            id="footer-button" style="display: block;">
            <a type="button" id="uploadfiles" href="javascript:;" data-ng-click="state.downloadZip()"
                class="btn black btn-xl"> Download </a>
        </div>
        <div class="footer-tools">

        </div>
    </div>
    <div id="processingStatus" style="display: initial" data-ng-show="state.loader">
        <div class="row">
            <div id="waiting">
                <span id="actionApp"></span> <br> <br>
                <img src="/images/svg_icons/preload.svg">

                <div id="waitnotify" class="alert alert-info"></div>
            </div>

        </div>
    </div>
</body>

</html>